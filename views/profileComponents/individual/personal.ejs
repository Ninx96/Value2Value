<div class="col-xl-10 offset-xl-1">
  <div class="dashboard-box">
    <!-- Headline -->
    <div class="headline">
      <h3><i class="icon-material-outline-face"></i> My Personal Details</h3>
    </div>

    <div class="content">
      <ul class="fields-ul">
        <li>
          <div class="row">
            <div class="col-md-6">
              <div class="submit-field">
                <h5>Address line 1</h5>
                <input
                  type="text"
                  class="with-border"
                  ="e.g. build me a website"
                />
              </div>
            </div>
            <div class="col-md-6">
              <div class="submit-field">
                <h5>Address line 2</h5>
                <input
                  type="text"
                  class="with-border"
                  ="e.g. build me a website"
                />
              </div>
            </div>
            <div class="col-md-4">
              <div class="submit-field">
                <h5>Pin Code</h5>
                <input
                  type="text"
                  class="with-border"
                  ="e.g. build me a website"
                />
              </div>
            </div>
            <div class="col-md-4">
              <div class="submit-field">
                <h5>City</h5>
                <select
                  class="selectpicker with-border"
                  data-size="7"
                  title="Select Category"
                >
                  <option>Chennai</option>
                  <option>Delhi</option>
                  <option>Mumbai</option>
                </select>
              </div>
            </div>
            <div class="col-md-4">
              <div class="submit-field">
                <h5>State</h5>
                <input
                  type="text"
                  class="with-border"
                  ="e.g. build me a website"
                />
              </div>
            </div>
          </div>
        </li>
        <li>
          <div class="row">
            <div class="col-md-5">
              <div class="submit-field">
                <h5>PAN Number</h5>
                <input
                  type="text"
                  class="with-border"
                  ="e.g. build me a website"
                />
              </div>
            </div>

            <div class="col-md-5 offset-xl-2">
              <div class="submit-field">
                <h5>GST Number</h5>
                <input
                  type="text"
                  class="with-border"
                  ="e.g. build me a website"
                />
              </div>
            </div>

            <div class="col-md-5">
              <div class="submit-field">
                <h5>Asset class you registered in</h5>
                <select
                  class="selectpicker with-border"
                  data-size="7"
                  title="Select Category"
                >
                  <option>Land and Building</option>
                  <option>Plant and Machinery</option>
                  <option>Security or Financial Assets</option>
                </select>
              </div>
            </div>

            <div class="col-md-5 offset-xl-2">
              <div class="submit-field">
                <h5>Preffered location for taking projects</h5>
                <select
                  class="selectpicker with-border"
                  data-size="7"
                  title="Select Category"
                >
                  <option>Chennai</option>
                  <option>Delhi</option>
                  <option>Mumbai</option>
                </select>
              </div>
            </div>
          </div>
        </li>
      </ul>
    </div>
  </div>
</div>
